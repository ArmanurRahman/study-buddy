{
  "expo": {
    "name": "Study Planner",
    "description": "A study planner app to help you manage your tasks and time effectively.",
    "slug": "my-expo-app",
    "version": "1.0.0",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [],
    "orientation": "portrait",
    "icon": "./assets/study-planner-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.my-expo-app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/study-planner-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.myexpoapp",
      "label": "Study Planner"
    },
    "extra": {
      "eas": {
        "projectId": "50c9c202-6df2-4de2-b30a-881287c624fe"
      }
    }
  }
}
